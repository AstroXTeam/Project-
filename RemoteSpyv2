local mkId = function()
	return tostring({}):gsub('table: ','')..tostring(function() end):gsub('function: ','').. tostring({}):gsub('table: ','')
end;

local RemoteInfo = {};
local InstanceEvent,InstanceFunction = Instance.new('RemoteEvent').FireServer,Instance.new('RemoteFunction').InvokeServer;
local FireServer , InvokeServer , Namecall;

local EventHook = function(self , ...)
	RemoteInfo[self] = RemoteInfo[self] or {};
	local blocked = RemoteInfo[self] and not RemoteInfo[self].Block;

	if not blocked then
		table.insert(RemoteInfo[self],{
			Script = getcallingscript() or debug.traceback(),
			Args = table.clone({...}),
			Self = self,
			method = "__index",
			Type = "FireServer",
			Id = mkId(),
		});
	end;
	
	if RemoteInfo[self].stop then
		return wait();
	end;
	
	return FireServer(self , ...);
end;

local InvokeHook = function(self , ...)
	RemoteInfo[self] = RemoteInfo[self] or {};
	
	local blocked = RemoteInfo[self] and not RemoteInfo[self].Block;
	
	if not blocked then
		table.insert(RemoteInfo[self],{
			Script = getcallingscript() or debug.traceback(),
			Args = table.clone({...}),
			Self = self,
			method = "__index",
			Type = "InvokeServer",
			Id = mkId(),
		});
	end;
	
	if RemoteInfo[self].stop then
		return wait();
	end;
	
	return InvokeServer(self , ...);
end;

FireServer = hookfunction(InstanceEvent,newcclosure(EventHook , 'FireServer'));
InvokeServer = hookfunction(InstanceFunction,newcclosure(InvokeHook , 'InvokeServer'));

Namecall = hookmetamethod(game , '__namecall' , newcclosure(function(self , ...)
	local args = {...};
	local method = getnamecallmethod();
	local caller = checkcaller();
	local call = getcallingscript();
	
	if method == 'FireServer' or method == 'InvokeServer' then
		RemoteInfo[self] = RemoteInfo[self] or {};
		
		if not RemoteInfo[self].Block then
			table.insert(RemoteInfo[self],{
				Script = call or debug.traceback(),
				Args = table.clone(args),
				Self = self,
				method = "__namecall",
				Type = method,
				caller = caller,
				Id = mkId(),
			});
		end;
		
		if RemoteInfo[self].stop then
			return wait();
		end;
	end;
	
	return Namecall(self,...)
end,'__namecall'));

local tableToLua;
local ObjectParent;

function ReverseTable(myTable)
	if myTable and #myTable > 1 then
		local temp = nil
		for n = 1,math.floor(#myTable/2) do
			temp = myTable[n]
			myTable[n] = myTable[#myTable-(n-1)]
			myTable[#myTable-(n-1)] = temp
		end 
	end
end

ObjectParent = function(instance,stack)
	stack = stack or {instance};
	task.wait();

	if instance and instance.Parent and instance.Parent ~= game then
		table.insert(stack,instance.Parent);	
		return ObjectParent(instance.Parent,stack)
	else
		return stack;
	end;
end;

local ParentParser = function(a)
	local stack = ObjectParent(a);
	local p = 'game';
	ReverseTable(stack);
	
	for i,v in ipairs(stack) do
		if i == 1 then
			p = p .. ':GetService("'..tostring(v)..'")'
		else
			if string.find(tostring(v),'\32',1,true) or string.find(tostring(v),'\t',1,true) or string.find(tostring(v),'\n',1,true) then
				p = p .. ':WaitForChild("'..tostring(v)..'"")'
			else
				p = p .. '.'..tostring(v);
			end;
		end;
	end;

	return p
end;

local Object = function(a)
	if typeof(a) == 'string' then
		local rel = string.gsub(string.gsub(a,'"','\\"'),'\n','\\n');
		return `"{rel}"`
	elseif typeof(a) == 'CFrame' then
		return `CFrame.new({tostring(a)})`
	elseif typeof(a) == 'Vector2' then
		return `Vector2.new({tostring(a)})`
	elseif typeof(a) == 'Vector3' then
		return `Vector3.new({tostring(a)})`
	elseif typeof(a) == 'UDim' then
		return `UDim.new({tostring(a.Offset)},{tostring(a.Scale)})`
	elseif typeof(a) == 'UDim2' then
		return `UDim2.new({tostring(a.X.Scale)},{tostring(a.X.Offset)},{tostring(a.Y.Scale)},{tostring(a.Y.Offset)})`
	elseif typeof(a) == 'Color3' then
		return `Color3.new({tostring(a)})`
	elseif typeof(a) == 'BrickColor' then
		return `BrickColor.new({tostring(a)})`
	elseif typeof(a) == 'buffer' then
		return `"Error buffer: {tostring(a)}"`
	elseif typeof(a) == 'number' or typeof(a) == 'boolean' then
		return tostring(a)
	elseif typeof(a) == 'function' then
		local s,l,n,a,f = debug.info(a,'slnaf');

		return `"function: {s} | {l} | {n} | {a} | {f}"`
	elseif typeof(a) == 'Instance' then
		return ParentParser(a);
	elseif typeof(a) == 'table' then
		return "TableStack{error}"
	end;
end;

tableToLua = function(Table, stack)
	if typeof(Table) ~= 'table' then
		return Object(Table) 
	end

	local aes = "{" 
	local tab = ("\t"):rep(stack or 0)

	stack = (stack or 0) + 1

	for i, v in next, Table do

		aes = aes .. '\n' .. tab .. '[' .. tostring(Object(i)) .. '] = ' .. tableToLua(v, stack) .. ','
	end


	return aes .. '\n' .. ("\t"):rep(stack - 1) .. '}'
end

spawn(function()
	local CodeBox = Highlight.new(CodeFrame);
	
	local GuiSelf = {};
	local FindFirstChild = clonefunction(game.FindFirstChild);
	local self_select = nil;
	local IdShow = nil;
	local debugs = false;
	local Infomation = {};
	
	BackButton.MouseButton1Click:Connect(function()
		IdShow = nil;
	end)
	
	local UIS = game:GetService('UserInputService')
	local dragToggle = nil
	local dragSpeed = 0.1
	local dragStart = nil
	local startPos = nil
	local rawcode = "";
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(Frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end

	headers.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = Frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)

	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
	
	GetCodeButton.MouseButton1Click:Connect(function()
		local a = setclipboard or toclipboard;a(rawcode);
	end)
	
	GetRemoteButton_2.MouseButton1Click:Connect(function()
		Infomation.Self[tostring(Infomation.Type)](Infomation.Self,unpack(Infomation.Args))
	end)
	
	GetScriptButton.MouseButton1Click:Connect(function()
		local a = setclipboard or toclipboard
		
		a(ParentParser(Infomation.Script))
	end);
	
	GetRemoteButton.MouseButton1Click:Connect(function()
		local a = setclipboard or toclipboard

		a(ParentParser(Infomation.Self))
	end);
	
	while true do task.wait();
		if IdShow then
			RemoteSelectFrame.Position = RemoteSelectFrame.Position:Lerp(UDim2.new(0.5,0,0.5,0),.25)
		else
			RemoteSelectFrame.Position = RemoteSelectFrame.Position:Lerp(UDim2.new(1.5,0,0.5,0),.25)
		end;
		
		if IdShow and debugs then
			debugs = false;
			local source = tableToLua(Infomation.Args);
			local rawcomment = `-- thread was created by executor: {Infomation.caller} , Method: {Infomation.method} , Id: {Infomation.Id}\n\n`
			rawcode = rawcomment..`local args = {source}\n\n{ParentParser(Infomation.Self)}:{tostring(Infomation.Type)}(unpack(args));`;
			CodeBox:setRaw(rawcode);
			
		end;
		
		table.foreach(RemoteInfo,function(index, self)
			local real_self = index;
			if not GuiSelf[real_self] then
				local Ref1 , Ref2 , Ref3 = CreateRemote(index.ClassName == 'RemoteEvent' and 'FireServer');
				local ControlPanel , ClearButton , BlockButton , StopButton = CreateControlPanel();
				
				GuiSelf[real_self] = {};
				GuiSelf[real_self].Gui = Ref1
				GuiSelf[real_self].RefName = Ref2
				GuiSelf[real_self].RefCout = Ref3
				GuiSelf[real_self].Scroll = MakeScroll();
				
				ControlPanel.Parent = GuiSelf[real_self].Scroll;
				ControlPanel.Name = 'CreateControlPanel';
				
				local del = tick();
				
				if not self_select then
					self_select = real_self;
				end;
				
				
				ClearButton.MouseButton1Click:Connect(function()
					RemoteInfo[real_self] = {};
					
					for i,v in ipairs(GuiSelf[real_self].Scroll:GetChildren()) do
						if v:IsA('Frame') then
							if v.Name ~= "CreateControlPanel" then
								v:Destroy();
							end;
						end;
					end;
				end);
				
				BlockButton.MouseButton1Click:Connect(function()
					RemoteInfo[real_self].Block = not RemoteInfo[real_self].Block
					if RemoteInfo[real_self].Block then
						BlockButton.Text = 'Unblock';
					else
						BlockButton.Text = 'Block';
					end;
				end);
				
				StopButton.MouseButton1Click:Connect(function()
					RemoteInfo[real_self].stop = not RemoteInfo[real_self].stop;
					
					if RemoteInfo[real_self].stop then
						StopButton.Text = 'Start';
					else
						StopButton.Text = 'Stop';
					end;
				end)
				
				Ref1.InputBegan:Connect(function(input: InputObject) 
					if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
						del = tick();
					end;
				end)
				
				Ref1.InputEnded:Connect(function(input: InputObject) 
					if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
						if (tick() - del) < 1 then
							self_select = real_self;
						end;
					end;
				end)
			else
				
				if self_select == real_self and not IdShow then
					GuiSelf[real_self].Scroll.Visible = true;
				else
					GuiSelf[real_self].Scroll.Visible = false;
				end;
				
				GuiSelf[real_self].RefName.Text = tostring(real_self);
				GuiSelf[real_self].RefCout.Text = tostring(#self);
				
				table.foreach(self,function(i,v)
					if not FindFirstChild(GuiSelf[real_self].Scroll , tostring(v.Id)) then
						local RemoteFrame , Button , ObjectId = makeObject();
						
						ObjectId.Text = tostring(v.Args);
						RemoteFrame.Parent = GuiSelf[real_self].Scroll;
						RemoteFrame.Name = tostring(v.Id);
						
						Button.MouseButton1Click:Connect(function()
							IdShow = self;
							debugs = true;
							Infomation = v;
						end)
					end;
				end)
			end;
		end);
	end;
end)
