local MakeScroll = function()
	local RemoteScrolling = Instance.new("ScrollingFrame")
	local UIListLayout = Instance.new("UIListLayout")

	RemoteScrolling.Name = "RemoteScrolling"
	RemoteScrolling.Parent = LabFrame
	RemoteScrolling.Active = true
	RemoteScrolling.AnchorPoint = Vector2.new(0.5, 0.5)
	RemoteScrolling.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	RemoteScrolling.BackgroundTransparency = 1.000
	RemoteScrolling.BorderColor3 = Color3.fromRGB(0, 0, 0)
	RemoteScrolling.BorderSizePixel = 0
	RemoteScrolling.ClipsDescendants = false
	RemoteScrolling.Position = UDim2.new(0.5, 0, 0.5, 0)
	RemoteScrolling.Size = UDim2.new(0.99000001, 0, 0.970000029, 0)
	RemoteScrolling.ZIndex = 4
	RemoteScrolling.ScrollBarThickness = 1
	
	UIListLayout:GetPropertyChangedSignal('AbsoluteContentSize'):Connect(function()
		RemoteScrolling.CanvasSize = UDim2.new(0,0,0,UIListLayout.AbsoluteContentSize.Y)
	end);
	
	UIListLayout.Parent = RemoteScrolling
	UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
	UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
	UIListLayout.Padding = UDim.new(0, 8)
	
	return RemoteScrolling
end;

